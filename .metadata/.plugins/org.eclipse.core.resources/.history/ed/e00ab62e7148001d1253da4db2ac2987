package com.boa.customerapi.services;

import java.util.List;

import com.boa.customerapi.models.Address;


public interface AddressService {
	Address addAddress(Address Address);
	Address updateAddress(long AddressId, String email, long contactNumber);
	List<Address> viewAllAddresss();
	Address viewAddressById(long AddressId);
	boolean deleteAddressById(long AddressId);
}
