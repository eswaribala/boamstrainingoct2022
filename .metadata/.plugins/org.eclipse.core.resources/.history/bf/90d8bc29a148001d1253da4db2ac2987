schema {
    query: Query  
  
}


interface Customer  {
    customerId: ID!
	name: FullName
	email: String
	password: String
	contactNumber: BigInt

}

type Individual implements Customer {
    customerId: ID!
	name: FullName
	email: String
	password: String
	contactNumber: BigInt
	gender:String
}




type FullName {

	firstName: String
	lastName: String
	middleName: String

}



type Query {
	findAllIndividuals:[Individual]
	findIndividual(customerId: ID):Individual
}


