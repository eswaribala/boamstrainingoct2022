interface Customer {
    customerId: ID!
	name: FullName
	email: String
	password: String
	contactNumber: long

}
type FullName {

	firstName: String
	lastName: String
	middleName: String

}

type Corporate implements Customer{
	
	companyType:String
}


extend type Query {
	findAllCorporates:[Corporate]
	findCorporate(customerId: ID):Corporate
}


